{
    "cli": {
        "version": ">= 5.2.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "channel": "production",
            "android": {
                "buildType": "apk",
                "image": "latest"
            },
            "env": {
                "NODE_VERSION": "20.10.0",
                "NODE_OPTIONS": "--max-old-space-size=8192"
            }
        }
    },
    "submit": {
        "production": {}
    }
}